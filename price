CREATE TABLE products(
	id int NOT NULL AUTO_INCREMENT,
	priceS CHAR(45),
	priceM CHAR(45),
	priceL CHAR(45),
	name CHAR(150),
	description CHAR(150),
	 PRIMARY KEY (id)
);
	
	
CREATE TABLE news(
	id int NOT NULL AUTO_INCREMENT,
	date datetime,
	title CHAR(45),
	description CHAR(150),
	 PRIMARY KEY (id)
);

CREATE TABLE user(
	id int NOT NULL AUTO_INCREMENT,
	login CHAR(100),
	password CHAR(100),
	 PRIMARY KEY (id)
);
